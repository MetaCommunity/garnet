#0Y UTF-8

(PROGN (SETQ *PACKAGE* (SYSTEM::%FIND-PACKAGE "OPAL")))
(PROGN (EXPORT '(STRING-SET-FUNC)))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'STRING-SET-FUNC)
 (SYSTEM::%PUTD 'STRING-SET-FUNC
  (SYSTEM::MAKE-MACRO
   (FUNCTION STRING-SET-FUNC
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 5.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET*
       ((GADGET-OBJ (CADR . #1=(SYSTEM::<MACRO-FORM>))) (STR-OBJ (CADDR . #1#))
        (FINAL-EVENT (CADDDR . #1#)) (FINAL-STRING (FIFTH . #1#)))
       (BLOCK STRING-SET-FUNC
        `(LET ((OBJ2 ,GADGET-OBJ))
          (KR-SEND OBJ2 :STRING-SET-FUNC OBJ2 ,STR-OBJ ,FINAL-EVENT
           ,FINAL-STRING))))))))))
(PROGN
 (SYSTEM::C-DEFUN 'SET-ONE-VALUE
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(GADGET-OBJ SLOT-NAME FINAL-STRING))))
(PROGN
 (SYSTEM::C-DEFUN 'DELETENTH (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(LST N))))
(PROGN
 (SYSTEM::C-DEFUN 'AGGRELIST-EDIT-STRING-FUNC
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE
   '(GADGET-OBJ AGGREL STR-OBJ FINAL-EVENT FINAL-STRING SLOT-FOR-RANK))))
(PROGN
 (SYSTEM::C-DEFUN 'MOTIF-BUTTON-STRING-FUNC
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE
   '(GADGET-OBJ STR-OBJ FINAL-EVENT FINAL-STRING))))
(PROGN
 (SYSTEM::C-DEFUN 'OPTION-BUTTON-STRING-FUNC
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE
   '(GADGET-OBJ STR-OBJ FINAL-EVENT FINAL-STRING))))