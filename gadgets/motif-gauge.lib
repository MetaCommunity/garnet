#0Y UTF-8

(PROGN (SETQ *PACKAGE* (SYSTEM::%FIND-PACKAGE "GARNET-GADGETS")))
(PROGN (EXPORT '(MOTIF-GAUGE))
 (EXPORT
  '(MOTIF-GAUGE-GO MOTIF-GAUGE-STOP MOTIF-GAUGE-WIN MOTIF-GAUGE-TOP-AGG
    DEMO-MOTIF-GAUGE)))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'SQR)
 (SYSTEM::%PUTD 'SQR
  (SYSTEM::MAKE-MACRO
   (FUNCTION SQR
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 2.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET* ((X (CADR SYSTEM::<MACRO-FORM>)))
       (BLOCK SQR
        (COND ((SYMBOLP X) `(* ,X ,X)) ((NUMBERP X) (* X X))
         (T
          (LET ((X-SYM (GENSYM)))
           `(LET ((,X-SYM ,X)) (* ,X-SYM ,X-SYM)))))))))))))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL LSTYLE-FLIP-ANGLE)))
(PROGN
 (SYSTEM::C-DEFUN 'STRING-SET-FUNC-METHOD-MOTIF-GAUGE
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE
   '(GADGET-OBJ STR-OBJ FINAL-EVENT FINAL-STRING))))
(PROGN
 (SYSTEM::C-DEFUN 'MOTIF-GAUGE-GO
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE
   '(&KEY DONT-ENTER-MAIN-EVENT-LOOP NOT-DOUBLE-BUFFERED-P))))
(PROGN
 (SYSTEM::C-DEFUN 'MOTIF-GAUGE-STOP (SYSTEM::LAMBDA-LIST-TO-SIGNATURE 'NIL)))