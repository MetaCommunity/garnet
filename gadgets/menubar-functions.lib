     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (< (LENGTH SYSTEM::<MACRO-FORM>) 4.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET*
       ((A-MENUBAR (CADR . #1=(SYSTEM::<MACRO-FORM>))) (B-ITEM (CADDR . #1#))
        (S-ITEM (CADDDR . #1#)) (ARGS (CDDDDR . #1#)))
       (BLOCK ADD-SUBMENU-ITEM
        `(LET ((MY-MENUBAR ,A-MENUBAR))
          (KR-SEND MY-MENUBAR :ADD-SUBMENU-ITEM MY-MENUBAR ,B-ITEM ,S-ITEM
           ,@ARGS))))))))))
