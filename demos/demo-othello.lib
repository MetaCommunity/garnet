 (SYSTEM::%PUTD 'OUT-OF-BOUNDS
  (SYSTEM::MAKE-MACRO
   (FUNCTION OUT-OF-BOUNDS
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 4.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET*
       ((ROW (CADR . #1=(SYSTEM::<MACRO-FORM>))) (COL (CADDR . #1#))
        (N (CADDDR . #1#)))
       (BLOCK OUT-OF-BOUNDS
        `(OR (>= ,ROW ,N) (>= ,COL ,N) (< ,ROW 0.) (< ,COL 0.))))))))))
