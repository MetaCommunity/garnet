(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'WITH-CURSOR)
 (SYSTEM::%PUTD 'WITH-CURSOR
  (SYSTEM::MAKE-MACRO
   (FUNCTION WITH-CURSOR
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (< (LENGTH SYSTEM::<MACRO-FORM>) 2.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET* ((CURSOR (CADR . #1=(SYSTEM::<MACRO-FORM>))) (BODY (CDDR . #1#)))
       (BLOCK WITH-CURSOR
        `(UNWIND-PROTECT (PROGN (CHANGE-CURSORS ,CURSOR) ,@BODY)
          (RESTORE-CURSORS))))))))))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'WITH-HOURGLASS-CURSOR)
 (SYSTEM::%PUTD 'WITH-HOURGLASS-CURSOR
  (SYSTEM::MAKE-MACRO
   (FUNCTION WITH-HOURGLASS-CURSOR
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (LET* ((BODY (CDR SYSTEM::<MACRO-FORM>)))
      (BLOCK WITH-HOURGLASS-CURSOR
       `(UNWIND-PROTECT (PROGN (CHANGE-CURSORS HOURGLASS-PAIR) ,@BODY)
         (RESTORE-CURSORS)))))))))
