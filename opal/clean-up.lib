#0Y UTF-8

(PROGN (SETQ *PACKAGE* (SYSTEM::%FIND-PACKAGE "OPAL")))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'ALREADY-BEEN-DESTROYED)
 (SYSTEM::%PUTD 'ALREADY-BEEN-DESTROYED
  (SYSTEM::MAKE-MACRO
   (FUNCTION ALREADY-BEEN-DESTROYED
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 2.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET* ((A-WINDOW (CADR SYSTEM::<MACRO-FORM>)))
       (BLOCK ALREADY-BEEN-DESTROYED `(NOT (SCHEMA-P ,A-WINDOW))))))))))
(PROGN
 (SYSTEM::C-DEFUN 'GET-TABLE-CONTENTS (SYSTEM::LAMBDA-LIST-TO-SIGNATURE 'NIL)))
(PROGN
 (SYSTEM::C-DEFUN 'CLEAN-UP
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(&OPTIONAL (HOW-TO :ORPHANS-ONLY)))))
(PROGN
 (SYSTEM::C-DEFUN 'CHANGE-GARNET-DISPLAY
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(NEW-DISPLAY))))
(PROGN
 (SYSTEM::C-DEFUN 'UPDATE-ALL
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(&OPTIONAL (TOTAL-P NIL)))))
(PROGN
 (SYSTEM::C-DEFUN 'RESET-CURSOR
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(A-WINDOW))))