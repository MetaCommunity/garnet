#0Y UTF-8

(PROGN (SETQ *PACKAGE* (SYSTEM::%FIND-PACKAGE "OPAL")))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'FAST-ERASE)
 (SYSTEM::%PUTD 'FAST-ERASE
  (SYSTEM::MAKE-MACRO
   (FUNCTION FAST-ERASE
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 5.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET*
       ((OBJECT (CADR . #1=(SYSTEM::<MACRO-FORM>))) (A-WINDOW (CADDR . #1#))
        (LINE-STYLE-GC (CADDDR . #1#)) (FILLING-STYLE-GC (FIFTH . #1#)))
       (BLOCK FAST-ERASE
        `(PROGN
          (GEM:SET-CLIP-MASK ,A-WINDOW :NONE ,LINE-STYLE-GC ,FILLING-STYLE-GC)
          (DRAW ,OBJECT ,A-WINDOW))))))))))
(PROGN
 (SYSTEM::C-DEFUN 'COPY-FROM-BUFFER-TO-DRAWABLE
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(A-WINDOW BBOX BUFFER DRAWABLE))))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'SWAP)
 (SYSTEM::%PUTD 'SWAP
  (SYSTEM::MAKE-MACRO
   (FUNCTION SWAP
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 3.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET* ((A (CADR . #1=(SYSTEM::<MACRO-FORM>))) (B (CADDR . #1#)))
       (BLOCK SWAP `(LET ((.TEMP. ,A)) (SETQ ,A ,B) (SETQ ,B .TEMP.))))))))))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL NEWLY-INVISIBLE-FASTDRAWS-BBOX)))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL FASTDRAW-OLD-BBOX)))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL PARENT-OLD-BBOX)))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL EXPOSED-CLIP-MASK)))
