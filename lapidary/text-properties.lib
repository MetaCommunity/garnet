(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'FORMULA-BUTTON)
 (SYSTEM::%PUTD 'FORMULA-BUTTON
  (SYSTEM::MAKE-MACRO
   (FUNCTION FORMULA-BUTTON
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (> (LENGTH SYSTEM::<MACRO-FORM>) 1.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET* NIL
       (BLOCK FORMULA-BUTTON
        `(G-VALUE TEXT-PROPERTIES-MENU :FONT-AGG :CONTENTS
          :FORMULA-BUTTON)))))))))
