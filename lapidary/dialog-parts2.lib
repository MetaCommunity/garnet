#0Y UTF-8

(PROGN (SETQ *PACKAGE* (SYSTEM::%FIND-PACKAGE "LAPIDARY")))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL *BOLD-FONT*)))
(PROGN (SYSTEM::C-PROCLAIM '(SPECIAL *VERY-LARGE-BOLD-ITALIC-SERIF-FONT*)))
(PROGN
 (SYSTEM::C-PROCLAIM
  '(SPECIAL TEXT-PROPERTIES-WIN NAME-BOX TITLED-FRAME TEXT-BOX SELECT-BOX
    ACT-BUTTONS FONT-PARAMETER-PANEL)))
(PROGN (SYSTEM::REMOVE-OLD-DEFINITIONS 'DIALOG-ENQUEUE)
 (SYSTEM::%PUTD 'DIALOG-ENQUEUE
  (SYSTEM::MAKE-MACRO
   (FUNCTION DIALOG-ENQUEUE
    (LAMBDA (SYSTEM::<MACRO-FORM> &OPTIONAL SYSTEM::<ENV-ARG>)
     (DECLARE (CONS SYSTEM::<MACRO-FORM>)) (DECLARE (IGNORE SYSTEM::<ENV-ARG>))
     (IF (/= (LENGTH SYSTEM::<MACRO-FORM>) 4.)
      (SYSTEM::MACRO-CALL-ERROR SYSTEM::<MACRO-FORM>)
      (LET*
       ((SLOT (CADR . #1=(SYSTEM::<MACRO-FORM>))) (VALUE (CADDR . #1#))
        (QUEUE (CADDDR . #1#)))
       (BLOCK DIALOG-ENQUEUE
        `(PROGN (SETF ,QUEUE (DELETE ,SLOT ,QUEUE :KEY #'CAR))
          (PUSH (CONS ,SLOT ,VALUE) ,QUEUE))))))))))
(PROGN
 (SYSTEM::C-DEFUN 'UNCONSTRAIN-FN
  (SYSTEM::LAMBDA-LIST-TO-SIGNATURE '(SLOT &KEY (TYPE NIL)))))